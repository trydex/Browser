<Window x:Class="Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button Content="Create Instance" Click="CreateInstance_Click" />
        <Button Content="Navigate to ya.ru" Click="Navigate_Click" />
        <Button Content="SendText" Click="SendText_Click" />
        <Button Content="Show Instances" Click="ShowInstances_Click" />
        <Button Content="Hide Instances" Click="HideInstances_Click" />
        <Button Content="Login to VK" Click="FindElement_Click" />

    </StackPanel>
</Window>
